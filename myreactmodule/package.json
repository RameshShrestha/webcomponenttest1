  },
  "devDependencies": {
    "bestzip": "^2.2.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.9",
    "npm-watch": "^0.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build1": "react-scripts build",
